<?xml version="1.0" encoding="utf-8" ?>
<!--

Purpose: This include file enables and configures the Publication foundation module.

This foundation module extends the standard publishing process by providing an option to publish referenced items.

To disable this file, change its extension to ".disabled".

-->
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <pipelines>
      <publishItem>
        <processor patch:instead="*[@type='Sitecore.XA.Foundation.Publication.Pipelines.PublishItem.CheckSharedItem, Sitecore.XA.Foundation.Publication']" type="Sitecore.Support.XA.Foundation.Publication.Pipelines.PublishItem.CheckSharedItem, Sitecore.Support.13071" resolve="true" />
      </publishItem>
    </pipelines>   
  </sitecore>
</configuration>